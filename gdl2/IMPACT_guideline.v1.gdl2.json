    "rules": {
      "gt0020": {
        "id": "gt0020",
        "priority": 6,
        "when": [
          "$gt0003|Age (Years)|!=null",
          "$gt0004|Motor Score|!=null",
          "$gt0005|Pupillary reactivity|!=null"
        ],
        "then": [
          "$gt0012|Core score |.magnitude=$gt0003.value+$gt0004.value+$gt0005.value"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 5,
        "when": [
          "$gt0006|Hypoxia|!=null",
          "$gt0007|Hypotension|!=null",
          "$gt0008|CT classification|!=null",
          "$gt0009|tSAH on CT|!=null",
          "$gt0010|Epidural mass on CT|!=null"
        ],
        "then": [
          "$gt0013|CT score|.magnitude=$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 4,
        "when": [
          "$gt0018|Glucose (mmol/l)|!=null",
          "$gt0019|Hb (g/dl)|!=null"
        ],
        "then": [
          "$gt0014|Lab score|.magnitude=$gt0018.value+$gt0019.value"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 3,
        "when": [
          "fired($gt0020)"
        ],
        "then": [
          "$gt0015|Lp mortality|.magnitude=(0.275*$gt0012.magnitude)-2.55"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 2,
        "when": [
          "fired($gt0020)"
        ],
        "then": [
          "$gt0016|Lp unfavorable outcome|.magnitude=(0.299*$gt0012.magnitude)-1.62"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "then": [
          "$gt0017|Probability of 6-month mortality/ unfavorable outcome|.magnitude=1/(1+exp($gt0015.magnitude))"
        ]
      }
    }
  },
