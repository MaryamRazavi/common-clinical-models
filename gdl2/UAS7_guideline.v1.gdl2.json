          }
        }
      },
      "gt0010": {
        "id": "gt0010",
        "model_id": "openEHR-EHR-OBSERVATION.uas7.v0",
        "template_id": "openEHR-EHR-OBSERVATION.uas7.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0011": {
            "id": "gt0011",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0011]"
          },
          "gt0014": {
            "id": "gt0014",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0012]"
          }
        }
      }
    },
    "rules": {
      "gt0012": {
        "id": "gt0012",
        "priority": 4,
        "when": [
          "$gt0003|UAS day 1|!=null",
          "$gt0004|UAS day 2|!=null",
          "$gt0005|UAS day 3|!=null",
          "$gt0006|UAS day 4|!=null",
          "$gt0007|UAS day 5|!=null",
          "$gt0008|UAS day 6|!=null",
          "$gt0009|UAS day 7|!=null"
        ],
        "then": [
          "$gt0011|UAS7|.magnitude=$gt0003.magnitude+$gt0004.magnitude+$gt0005.magnitude+$gt0006.magnitude+$gt0007.magnitude+$gt0008.magnitude+$gt0009.magnitude"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 3,
        "when": [
          "$gt0011|UAS7|<='6'"
        ],
        "then": [
          "$gt0014|Interpretation|='Mild urticaria '"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 2,
        "when": [
          "$gt0011|UAS7|>'6'",
          "$gt0011|UAS7|<'28'"
        ],
        "then": [
          "$gt0014|Interpretation|='Moderate urticaria '"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 1,
        "when": [
          "$gt0011|UAS7|>='28'"
        ],
        "then": [
          "$gt0014|Interpretation|='Severe urticaria '"
        ]
      }
    }
  },
